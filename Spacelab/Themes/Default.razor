@namespace Oqtane.Theme.Bootswatch.Spacelab
@inherits Oqtane.Theme.Bootswatch.Default.Default

@{ 
    base.BuildRenderTree(__builder); 
}

@code {

    public override List<Resource> Resources => GetResources();

    private List<Resource> GetResources()
    {
        var resources = base.Resources;
        resources[0] = new Resource { ResourceType = ResourceType.Stylesheet, Url = ThemePath().Replace(".Spacelab/", "/Spacelab.css") };
        resources[1] = new Resource { ResourceType = ResourceType.Stylesheet, Url = ThemePath().Replace(".Spacelab/", "/Theme.css") };
        return resources;
    }
}
